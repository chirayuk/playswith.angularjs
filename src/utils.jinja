{% macro insert_ng_templates(ngTemplateNames) %}
  {% for ngTemplateName in ngTemplateNames %}
    <script type="text/ng-template" id="[[ngTemplateName]]">
      [[ get_file_contents("src/" + ngTemplateName) ]]
    </script>
  {% endfor %}
{% endmacro %}
